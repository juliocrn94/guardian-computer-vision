    "def calc_dist_xy(array, k =1.2):\n",
    "    dH1 = array[0,0]-array[1,0]\n",
    "    dH2 = array[0,2]-array[1,2]\n",
    "    dH_av = abs(mean([dH1,dH2]))\n",
    "\n",
    "    dW1 = array[0,0]-array[1,0]\n",
    "    dW2 = array[0,2]-array[1,2]\n",
    "    dW_av = abs(mean([dW1,dW2]))\n",
    "\n",
    "    dR = math.sqrt(dH_av**2+dW_av**2)\n",
    "    return dR"
