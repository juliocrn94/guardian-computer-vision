    "def calc_dist_xz(array, k =1.2):\n",
    "    dH1 = array[0,0]-array[1,0]\n",
    "    dH2 = array[0,2]-array[1,2]\n",
    "    dH_av = abs(mean([dH1,dH2]))\n",
    "\n",
    "    h1 = abs(array[0,1]-array[0,3])\n",
    "    h2 = abs(array[1,1]-array[1,3])\n",
    "\n",
    "\n",
    "    dZ1 = (h1/h2)*k\n",
    "    dZ2 = (h2/h1)*k\n",
    "    dZ_max = max(dZ1,dZ2)\n",
    "\n",
    "    dR = math.sqrt(dZ_max**2+dH_av**2)\n",
    "    return dR"
   ]
